class Solution:
    def search(self, nums: List[int], target: int) -> int:
        if target not in nums:
            return -1
        
        i = (len(nums) - 1) // 2
        j = 0
        k = len(nums -1)
        while nums[i] != target:
            if target > nums[i]:
                j = i   #previous i if next i bigger
                i += (k - i)/ 2
        else:
                i -= (len(List[j:i] // 2)
                k = i   #previous i if next smaller
            
        return i
